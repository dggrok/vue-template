{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "基于pnpm管理的多包项目，包含vue 中后台模版，vue组件库，公共utils库，以及文档平台",
  "private": true,
  "scripts": {
    "components:dev": "pnpm --filter @cyw/vue-components dev",
    "components:build": "pnpm --filter @cyw/vue-components build",
    "docs:dev": "pnpm --filter @cyw/vue-components-docs dev",
    "docs:build": "pnpm --filter @cyw/vue-components-docs build",
    "admin:dev": "pnpm --filter @cyw/vue-admin dev",
    "build": "node scripts/build.js",
    "clean": "node scripts/clean.js",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dggrok/vue-template.git"
  },
  "keywords": [
    "lerna",
    "vue"
  ],
  "author": "yaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dggrok/vue-template/issues"
  },
  "homepage": "https://github.com/dggrok/vue-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "7.16.0",
    "eslint-config-vue2": "^1.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "stylelint": "13.13.1"
  }
}
