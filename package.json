{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "基于lerna管理的多包项目，包含vue 中后台模版，vue组件库，公共utils库，以及文档平台",
  "private": true,
  "workspaces": {
    "packages": [
      "docs/*",
      "packages/*",
      "projects/*"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dggrok/vue-template.git"
  },
  "keywords": [
    "lerna",
    "vue"
  ],
  "author": "yaw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dggrok/vue-template/issues"
  },
  "homepage": "https://github.com/dggrok/vue-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.4",
    "stylelint": "^14.5.0"
  }
}
